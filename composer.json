{
    "name": "monolog-http/monolog-http-bundle",
    "type": "symfony-bundle",
    "description": "monolog-http-bundle",
    "keywords": [
        "monolog-http-bundle",
        "bundle",
        "symfony"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "George Mponos",
            "email": "gmponos@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/config": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/http-kernel": "^4.3 || ^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.30",
        "phpstan/phpstan-phpunit": "^0.12.11",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "phpunit/phpunit": "^8.5.7 || ^9.3.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "~3.5.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MonologHttp\\MonologHttpBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonologHttp\\MonologHttpBundle\\Tests\\": "tests"
        }
    }
}